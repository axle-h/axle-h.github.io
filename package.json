{
  "name": "axle-h.github.io",
  "version": "1.0.0",
  "description": "Jekyll static website hosted on ax-h.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axle-h/axle-h.github.io.git"
  },
  "author": "Alex Haslehurst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/axle-h/axle-h.github.io/issues"
  },
  "homepage": "https://github.com/axle-h/axle-h.github.io#readme",
  "scripts": {
    "build": "npm-run-all --serial webpack copy-assets copy-templates",
    "webpack": "webpack",
    "copy-assets": "copy 'dist/assets/**/*' assets",
    "copy-templates": "copy 'dist/*.html' _includes",
    "watch": "nodemon --watch 'src/**/*' -e scss,js,html --exec 'npm run build'"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "materialize-css": "^1.0.0-alpha.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy": "^0.3.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "sass-loader": "^6.0.6",
    "webpack": "^3.8.1"
  }
}
